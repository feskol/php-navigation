{
    "name": "feskol/php-navigation",
    "description": "A simple navigation class",
    "type": "library",
    "keywords": [
        "navigation",
        "navigation-active-state",
        "navigation-helper"
    ],
    "authors": [
        {
            "name": "Festim Kolgeci",
            "email": "festim.kolgeci@pm.me"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Feskol\\Navigation\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Feskol\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
